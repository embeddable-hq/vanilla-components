{
  "name": "vanilla-components",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "embeddable:build": "embeddable build",
    "embeddable:push": "embeddable push",
    "embeddable:login": "embeddable login",
    "embeddable:dev": "cross-env NODE_OPTIONS='--max-old-space-size=4096' embeddable dev",
    "cube:playground": "docker-compose -f ./src/cube-playground.yml up",
    "cube:cubestore": "mysql -h 127.0.0.1 -P 13306 --user=cubestore -pcubestore",
    "dev": "cross-env NODE_OPTIONS='--max-old-space-size=4096' embeddable dev",
    "build": "embeddable build",
    "push": "embeddable push",
    "lint": "npx eslint . --ext .ts --ext .tsx"
  },
  "dependencies": {
    "@cubejs-backend/api-gateway": "^0.34.31",
    "@headlessui/tailwindcss": "^0.2.0",
    "@types/react-simple-maps": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "chart.js": "^4.4.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "chrono-node": "^2.7.3",
    "d3-scale": "^4.0.2",
    "date-fns": "^2.30.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-day-picker": "^8.9.1",
    "react-dom": "^18.2.0",
    "react-simple-maps": "^3.0.0",
    "tailwind-merge": "^2.2.1",
    "vite-plugin-css-injected-by-js": "^3.1.1"
  },
  "overrides": {
    "react-simple-maps": {
      "d3-zoom": "^3.0.0",
      "d3-interpolate": "^3.0.1",
      "d3-color": "^3.1.0"
    },
    "@cubejs-backend/api-gateway": {
      "jsonwebtoken": "^9.0.0"
    }
  },
  "devDependencies": {
    "@embeddable.com/core": "^2.5.5",
    "@embeddable.com/react": "^2.5.5",
    "@embeddable.com/sdk-core": "^2.5.5",
    "@embeddable.com/sdk-react": "^2.3.5",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.4.24",
    "prettier": "3.1.1",
    "tailwindcss": "^3.3.2",
    "tw-colors": "^2.2.0",
    "typescript": "^5.3.3",
    "vite": "^4.3.9",
    "vite-plugin-singlefile": "^0.13.5"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2020": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime",
      "plugin:react-hooks/recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "settings": {
      "react": {
        "version": "18.2"
      }
    },
    "plugins": [
      "react-refresh",
      "@typescript-eslint"
    ],
    "rules": {
      "react/display-name": "off",
      "react/prop-types": "off",
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "react-refresh/only-export-components": "off"
    }
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 100,
    "trailingComma": "none",
    "endOfLine": "auto",
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrder": [
      "^(?!@/)(?!\\.).+",
      "."
    ]
  }
}
